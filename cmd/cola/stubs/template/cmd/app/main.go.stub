package main

import (
	_ "{{.}}/internal/boot"

	"github.com/spf13/viper"
	"github.com/zedisdog/cola"
	"{{.}}/internal/controllers"
	"{{.}}/internal/database"
	"{{.}}/internal/log"
)

func main() {
	server := cola.NewServer(
		viper.GetViper(),
		controllers.NewRoutes(),
		database.Instance(),
		log.GetInstance(),
		database.Migrations,
	)
	server.Start()
}
