package main

import (
	"github.com/spf13/viper"
	"github.com/zedisdog/cola"
	"{{.}}"
	"{{.}}/internal/controllers"
	"{{.}}/internal/database"
	"{{.}}/internal/log"
)

func init() {
	cola.Config({{.}}.Config)
}

func main() {
	server := cola.NewServer(
		viper.GetViper(),
		controllers.NewRoutes(),
		database.GetInstance(),
		log.GetInstance(),
		database.Migrations,
	)
	server.Start()
}
