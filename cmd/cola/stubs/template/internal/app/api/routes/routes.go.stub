package routes

import (
	"github.com/gin-gonic/gin"
	"github.com/zedisdog/cola/transport/http/chttp"
	"github.com/zedisdog/cola/transport/http/middlewares"
	"{{.}}/internal/app/api"
)

func init() {
	r := gin.Default()
	r.Use(middlewares.Cros)

	r.GET("test", api.Test.Index)
	r.POST("login", api.Account.Login)

	chttp.Server().SetRouter(r)
}
